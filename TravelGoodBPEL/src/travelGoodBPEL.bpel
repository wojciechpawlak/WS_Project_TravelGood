<?xml version="1.0" encoding="UTF-8"?>
<process
    name="travelGoodBPEL"
    targetNamespace="http://enterprise.netbeans.org/bpel/TravelGoodBPEL/travelGoodBPEL"
    xmlns:tns="http://enterprise.netbeans.org/bpel/TravelGoodBPEL/travelGoodBPEL"
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"
    xmlns:sxt="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Trace" 
    xmlns:sxed="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor"
    xmlns:sxeh="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/ErrorHandling" xmlns:sxed2="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/Editor2" xmlns:ns0="http://travelgood.ws" xmlns:sxxf="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension/XPathFunctions" xmlns:ns1="http://www.sun.com/wsbpel/2.0/process/executable/SUNExtension" ns1:ignoreMissingFromData="yes" xmlns:ns2="http://types.niceview.ws" xmlns:ns3="http://types.lameduck.ws" xmlns:ns4="http://docs.oasis-open.org/wsbpel/2.0/process/executable">
    <import namespace="http://travelgood.ws" location="TravelGoodWSDL.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/LameDuckWSDLServiceWrapper" location="LameDuckWSDLServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://lameduck.ws" location="http://localhost:8080/LameDuckService/LameDuckWSDLService?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://enterprise.netbeans.org/bpel/NiceViewWSDLServiceWrapper" location="NiceViewWSDLServiceWrapper.wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <import namespace="http://niceview.ws" location="http://localhost:8080/NiceViewService/NiceViewWSDLService?wsdl" importType="http://schemas.xmlsoap.org/wsdl/"/>
    <partnerLinks>
        <partnerLink name="PartnerLink2" xmlns:tns="http://enterprise.netbeans.org/bpel/LameDuckWSDLServiceWrapper" partnerLinkType="tns:LameDuckWSDLLinkType" partnerRole="LameDuckWSDLRole"/>
        <partnerLink name="PartnerLink3" xmlns:tns="http://enterprise.netbeans.org/bpel/NiceViewWSDLServiceWrapper" partnerLinkType="tns:NiceViewWSDLLinkType" partnerRole="NiceViewWSDLRole"/>
        <partnerLink name="PartnerLink1" xmlns:tns="http://travelgood.ws" partnerLinkType="tns:TravelGoodWSDL" myRole="TravelGoodWSDLPortTypeRole"/>
    </partnerLinks>
    <variables>
        <variable name="doneProcess" type="xsd:boolean"/>
        <variable name="CancelItineraryOut" messageType="ns0:cancelItineraryResponse"/>
        <variable name="CancelItineraryIn" messageType="ns0:cancelItineraryRequest"/>
        <variable name="BookHotelOut" xmlns:tns="http://niceview.ws" messageType="tns:bookHotelResponse"/>
         <variable name="BookHotelIn" xmlns:tns="http://niceview.ws" messageType="tns:bookHotelRequest"/>
         <variable name="BookFlightsOut" xmlns:tns="http://lameduck.ws" messageType="tns:bookFlightResponse"/>
         <variable name="BookFlightsIn" xmlns:tns="http://lameduck.ws" messageType="tns:bookFlightRequest"/>
         <variable name="arrayFlightSize" type="xsd:int"/>
         <variable name="arrayHotelSize" type="xsd:int"/>
        <variable name="AddFlightOut" messageType="ns0:addFlightResponse"/>
          <variable name="AddFlightIn" messageType="ns0:addFlightRequest"/>
          <variable name="GetFlightsOut" xmlns:tns="http://lameduck.ws" messageType="tns:getFlightsResponse"/>
        <variable name="GetFlightsIn" xmlns:tns="http://lameduck.ws" messageType="tns:getFlightsRequest"/>
        <variable name="GetFlightOut" messageType="ns0:getFlightResponse"/>
        <variable name="GetFlightIn" messageType="ns0:getFlightRequest"/>
        <variable name="BookingItineraryOut" messageType="ns0:bookingItineraryResponse"/>
        <variable name="BookingItineraryIn" messageType="ns0:bookingItineraryRequest"/>
        <variable name="donePlanning" type="xs:boolean"/>
        <variable name="GetItineraryOut" messageType="ns0:getItineraryResponse"/>
        <variable name="GetItineraryIn" messageType="ns0:getItineraryRequest"/>
        <variable name="changeItineraryStartDate" type="xs:boolean"/>
        <variable name="Itinerary" type="ns0:itineraryType">
            <sxed2:editor>
                <sxed2:predicate path="$Itinerary/bookingsHotel[$arrayHotelSize]" source="to"/>
                 <sxed2:predicate path="$Itinerary/bookingsFlight[$arrayFlightSize]" source="to"/>
                <sxed2:predicate path="$Itinerary/bookingsFlight[$ForEach1Counter]" source="from"></sxed2:predicate>
                <sxed2:predicate path="$Itinerary/bookingsFlight[$ForEach1Counter]" source="to"/>
                <sxed2:predicate path="$Itinerary/bookingsHotel[$ForEach2Counter]" source="from"/>
                <sxed2:predicate path="$Itinerary/bookingsHotel[$ForEach2Counter]" source="to"/>
            </sxed2:editor>
        </variable>
        <variable name="AddHotelOut" messageType="ns0:addHotelResponse"/>
        <variable name="AddHotelIn" messageType="ns0:addHotelRequest"/>
        <variable name="GetHotelsOut" xmlns:tns="http://niceview.ws" messageType="tns:getHotelsResponse"/>
        <variable name="GetHotelsIn" xmlns:tns="http://niceview.ws" messageType="tns:getHotelsRequest"/>
        <variable name="GetHotelOut" xmlns:tns="http://travelgood.ws" messageType="tns:getHotelResponse"></variable>
        <variable name="GetHotelIn" xmlns:tns="http://travelgood.ws" messageType="tns:getHotelRequest"/>
        <variable name="CreateItineraryOut" xmlns:tns="http://travelgood.ws" messageType="tns:createItineraryResponse"/>
        <variable name="CreateItineraryIn" xmlns:tns="http://travelgood.ws" messageType="tns:createItineraryRequest"/>
    </variables>
    <correlationSets>
        <correlationSet name="CustomerItineraryCorrelation" properties="ns0:itineraryId ns0:customerId"/>
    </correlationSets>
    <sequence>
        <receive name="Receive1" createInstance="yes" partnerLink="PartnerLink1" operation="createItinerary" xmlns:tns="http://travelgood.ws" portType="tns:TravelGoodWSDLPortType" variable="CreateItineraryIn">
            <correlations>
                <correlation set="CustomerItineraryCorrelation" initiate="yes"/>
            </correlations>
        </receive>
        <assign name="Assign1">
            <copy>
                <from>'Itinerary Created'</from>
                <to variable="CreateItineraryOut" part="confirmation"/>
            </copy>
            <copy>
                <from>false()</from>
                <to variable="donePlanning"/>
            </copy>
            <copy>
                <from>'3000-01-01T00:00:0.0+01:00'</from>
                <to>$Itinerary/itineraryStartDate</to>
            </copy>
            <copy>
                <from>0</from>
                <to variable="arrayHotelSize"/>
            </copy>
             <copy>
                  <from>0</from>
                  <to variable="arrayFlightSize"/>
             </copy>
            <copy>
                <from>false()</from>
                <to variable="doneProcess"/>
            </copy>
        </assign>
        <reply name="Reply1" partnerLink="PartnerLink1" operation="createItinerary" xmlns:tns="http://travelgood.ws" portType="tns:TravelGoodWSDLPortType" variable="CreateItineraryOut"/>
        <repeatUntil name="RepeatUntil1">
            <pick name="Pick1" xmlns:tns="http://niceview.ws">
                <documentation>KB : Maybe add an if to each onMessage, to separate the behaviour if donePlanning = true or false</documentation>
                <onMessage partnerLink="PartnerLink1" operation="getHotel" portType="ns0:TravelGoodWSDLPortType" variable="GetHotelIn">
                    <documentation>getHotel operation</documentation>
                    <correlations>
                                <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                            </correlations>
                            <sequence name="Sequence1">
                                <assign name="Assign2">
                                        <copy>
                                                <from variable="GetHotelIn" part="city"/>
                                                    <to variable="GetHotelsIn" part="city"/>
                                            </copy>
                                            <copy>
                                                <from variable="GetHotelIn" part="arrivalDate"/>
                                                    <to variable="GetHotelsIn" part="arrivalDate"/>
                                            </copy>
                                            <copy>
                                                <from variable="GetHotelIn" part="departureDate"/>
                                                    <to variable="GetHotelsIn" part="departureDate"/>
                                            </copy>
                                </assign>
                                 <scope name="Scope3">
                                      <eventHandlers>
                                           <onAlarm>
                                                <for>'P0Y0M0DT0H0M5.0S'</for>
                                                <scope name="Scope4">
                                                     <sequence name="Sequence11">
                                                          <assign name="Assign20">
                                                               <copy>
                                                                    <from>
                                                                         <literal><ns:hotelList
    xmlns:ns="http://types.niceview.ws"
    zmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></literal>
                                                                    </from>
                                                                    <to variable="GetHotelOut" part="hotelList"/>
                                                               </copy>
                                                          </assign>
                                                          <reply name="Reply9" partnerLink="PartnerLink1" operation="getHotel" portType="ns0:TravelGoodWSDLPortType" variable="GetHotelOut"/>
                                                     </sequence>
                                                </scope>
                                           </onAlarm>
                                      </eventHandlers>
                                      <invoke name="Invoke1" partnerLink="PartnerLink3" operation="getHotels" portType="tns:NiceViewWSDLPortType" inputVariable="GetHotelsIn" outputVariable="GetHotelsOut"/>
                                 </scope>
                                 <assign name="Assign3">
                                          <copy>
                                                <from variable="GetHotelsOut" part="hotels"/>
                                                <to variable="GetHotelOut" part="hotelList"/>
                                          </copy>
                                    </assign>
                                    <reply name="Reply2" partnerLink="PartnerLink1" operation="getHotel" portType="ns0:TravelGoodWSDLPortType" variable="GetHotelOut"/>
                            </sequence>
                    </onMessage>
                    <onMessage partnerLink="PartnerLink1" operation="addHotel" portType="ns0:TravelGoodWSDLPortType" variable="AddHotelIn">
                        <documentation>addHotel operation</documentation>
                        <correlations>
                                <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                            </correlations>
                            <sequence name="Sequence2">
                                <assign name="Assign13">
                                     <copy>
                                          <from>$arrayHotelSize + 1</from>
                                          <to variable="arrayHotelSize"/>
                                     </copy>
                                </assign>
                                <assign name="Assign4">
                                        <copy>
                                                <from>'Hotel added'</from>
                                                    <to variable="AddHotelOut" part="confirmation"/>
                                            </copy>
                                    <copy>
                                        <from>'unconfirmed'</from>
                                        <to>$Itinerary/bookingsHotel[$arrayHotelSize]/bookingStatus
                                            <sxed2:editor>
                                                <sxed2:predicate path="$Itinerary/bookingsHotel[$arrayHotelSize]" source="to"/>
                                            </sxed2:editor>
                                        </to>
                                    </copy>
                                     <copy>
                                          <from variable="AddHotelIn" part="bookingNumber"/>
                                          <to>$Itinerary/bookingsHotel[$arrayHotelSize]/bookingNumber
                                               <sxed2:editor>
                                                    <sxed2:predicate path="$Itinerary/bookingsHotel[$arrayHotelSize]" source="to"/>
                                               </sxed2:editor>
                                          </to>
                                     </copy>
                                    <copy>
                                        <from>sxxf:date-less-than($GetHotelIn.arrivalDate, $Itinerary/itineraryStartDate)</from>
                                        <to variable="changeItineraryStartDate"/>
                                    </copy>
                                </assign>
                                <if name="If1">
                                        <condition>$changeItineraryStartDate</condition>
                                            <assign name="Assign5">
                                                  <copy>
                                                        <from variable="GetHotelIn" part="arrivalDate"/>
                                                        <to>$Itinerary/itineraryStartDate</to>
                                                  </copy>
                                            </assign>
                                    </if>
                                    <reply name="Reply3" partnerLink="PartnerLink1" operation="addHotel" portType="ns0:TravelGoodWSDLPortType" variable="AddHotelOut"/>
                            </sequence>
                    </onMessage>
                    <onMessage partnerLink="PartnerLink1" operation="getItinerary" portType="ns0:TravelGoodWSDLPortType" variable="GetItineraryIn">
                        <documentation>getItinerary operation</documentation>
                        <correlations>
                            <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                        </correlations>
                            <sequence name="Sequence3">
                                <assign name="Assign6">
                                        <copy>
                                                <from variable="Itinerary"/>
                                                    <to variable="GetItineraryOut" part="itinerary"/>
                                            </copy>
                                    </assign>
                                    <reply name="Reply4" partnerLink="PartnerLink1" operation="getItinerary" portType="ns0:TravelGoodWSDLPortType" variable="GetItineraryOut"/>
                            </sequence>
                    </onMessage>
                <onMessage partnerLink="PartnerLink1" operation="bookingItinerary" portType="ns0:TravelGoodWSDLPortType" variable="BookingItineraryIn">
                    <documentation>bookingItinerary operation</documentation>
                    <correlations>
                        <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                    </correlations>
                    <sequence name="Sequence4">
                        <forEach name="ForEach1" parallel="no" counterName="ForEach1Counter">
                            <startCounterValue>1</startCounterValue>
                            <finalCounterValue>$arrayFlightSize</finalCounterValue>
                            <scope name="Scope1">
                                <sequence name="Sequence7">
                                    <if name="If3">
                                        <condition>$Itinerary/bookingsFlight[$ForEach1Counter]/bookingNumber != 'null'
                                            <sxed2:editor>
                                                <sxed2:predicate path="$Itinerary/bookingsFlight[$ForEach1Counter]" source="from"/>
                                            </sxed2:editor>
                                        </condition>
                                        <sequence name="Sequence8">
                                            <assign name="Assign15">
                                                <copy>
                                                    <from variable="BookingItineraryIn" part="creditCardInfo"/>
                                                        <to>$BookFlightsIn.input/ns3:creditCardInformation</to>
                                                </copy>
                                                    <copy>
                                                        <from>$Itinerary/bookingsFlight[$ForEach1Counter]/bookingNumber
                                                                    <sxed2:editor>
                                                                        <sxed2:predicate path="$Itinerary/bookingsFlight[$ForEach1Counter]" source="from"/>
                                                                    </sxed2:editor>
                                                            </from>
                                                            <to>$BookFlightsIn.input/ns3:bookingNumber</to>
                                                    </copy>
                                            </assign>
                                            <invoke name="Invoke3" partnerLink="PartnerLink2" operation="bookFlights" xmlns:tns="http://lameduck.ws" portType="tns:LameDuckWSDLPortType" inputVariable="BookFlightsIn" outputVariable="BookFlightsOut"/>
                                            <assign name="Assign8">
                                                <copy>
                                                    <from>'confirmed'</from>
                                                    <to>$Itinerary/bookingsFlight[$ForEach1Counter]/bookingStatus
                                                        <sxed2:editor>
                                                            <sxed2:predicate path="$Itinerary/bookingsFlight[$ForEach1Counter]" source="to"/>
                                                        </sxed2:editor>
                                                    </to>
                                                </copy>
                                            </assign>
                                        </sequence>
                                    </if>
                                </sequence>
                            </scope>
                        </forEach>
                        <forEach name="ForEach2" parallel="no" counterName="ForEach2Counter">
                            <startCounterValue>1</startCounterValue>
                            <finalCounterValue>$arrayHotelSize</finalCounterValue>
                            <scope name="Scope2">
                                <if name="If4">
                                    <condition>$Itinerary/bookingsHotel[$ForEach2Counter]/bookingNumber != 'null'
                                        <sxed2:editor>
                                            <sxed2:predicate path="$Itinerary/bookingsHotel[$ForEach2Counter]" source="from"/>
                                        </sxed2:editor>
                                    </condition>
                                    <sequence name="Sequence9">
                                        <assign name="Assign17">
                                            <copy>
                                                <from>$Itinerary/bookingsHotel[$ForEach2Counter]/bookingNumber
                                                    <sxed2:editor>
                                                        <sxed2:predicate path="$Itinerary/bookingsHotel[$ForEach2Counter]" source="from"/>
                                                    </sxed2:editor>
                                                </from>
                                                <to variable="BookHotelIn" part="bookingNumber"/>
                                            </copy>
                                            <copy>
                                                <from>$BookingItineraryIn.creditCardInfo/expirationDate</from>
                                                <to>$BookHotelIn.creditCardInfo/expirationDate</to>
                                            </copy>
                                            <copy>
                                                <from>$BookingItineraryIn.creditCardInfo/name</from>
                                                <to>$BookHotelIn.creditCardInfo/name</to>
                                            </copy>
                                            <copy>
                                                <from>$BookingItineraryIn.creditCardInfo/number</from>
                                                <to>$BookHotelIn.creditCardInfo/number</to>
                                            </copy>
                                        </assign>
                                        <invoke name="Invoke4" partnerLink="PartnerLink3" operation="bookHotel" portType="tns:NiceViewWSDLPortType" inputVariable="BookHotelIn" outputVariable="BookHotelOut"/>
                                        <assign name="Assign18">
                                            <copy>
                                                <from>'confirmed'</from>
                                                <to>$Itinerary/bookingsHotel[$ForEach2Counter]/bookingStatus
                                                    <sxed2:editor>
                                                        <sxed2:predicate path="$Itinerary/bookingsHotel[$ForEach2Counter]" source="to"/>
                                                    </sxed2:editor>
                                                </to>
                                            </copy>
                                        </assign>
                                    </sequence>
                                </if>
                            </scope>
                        </forEach>
                        <assign name="Assign16">
                            <copy>
                                <from>'Booking done'</from>
                                <to variable="BookingItineraryOut" part="bookingValidation"/>
                            </copy>
                            <copy>
                                <from>true()</from>
                                <to variable="donePlanning"/>
                            </copy>
                        </assign>
                        <reply name="Reply5" partnerLink="PartnerLink1" operation="bookingItinerary" portType="ns0:TravelGoodWSDLPortType" variable="BookingItineraryOut"/>
                    </sequence>
                </onMessage>
                <onMessage partnerLink="PartnerLink1" operation="getFlight" portType="ns0:TravelGoodWSDLPortType" variable="GetFlightIn">
                    <documentation>getFlight operation</documentation>
                    <correlations>
                        <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                    </correlations>
                    <sequence name="Sequence5">
                        <assign name="Assign9">
                            <copy>
                                <from variable="GetFlightIn" part="flightStart"/>
                                <to>$GetFlightsIn.input/ns3:flightStart</to>
                            </copy>
                            <copy>
                                <from variable="GetFlightIn" part="flightDestination"/>
                                <to>$GetFlightsIn.input/ns3:flightDestination</to>
                            </copy>
                            <copy>
                                <from variable="GetFlightIn" part="flightDate"/>
                                <to>$GetFlightsIn.input/ns3:flightDate</to>
                            </copy>
                        </assign>
                         <scope name="Scope5">
                              <eventHandlers>
                                   <onAlarm>
                                        <for>'P0Y0M0DT0H0M5.0S'</for>
                                        <scope name="Scope6">
                                             <sequence name="Sequence12">
                                                  <assign name="Assign21">
                                                       <copy>
                                                            <from>
                                                                 <literal><ns0:flightList
    xmlns:ns0="http://types.lameduck.ws"
    zmlns="http://docs.oasis-open.org/wsbpel/2.0/process/executable"/></literal>
                                                            </from>
                                                            <to variable="GetFlightOut" part="flightList"/>
                                                       </copy>
                                                  </assign>
                                                  <reply name="Reply10" partnerLink="PartnerLink1" operation="getFlight" portType="ns0:TravelGoodWSDLPortType" variable="GetFlightOut"/>
                                             </sequence>
                                        </scope>
                                   </onAlarm>
                              </eventHandlers>
                              <invoke name="Invoke2" partnerLink="PartnerLink2" operation="getFlights" xmlns:tns="http://lameduck.ws" portType="tns:LameDuckWSDLPortType" inputVariable="GetFlightsIn" outputVariable="GetFlightsOut"/>
                         </scope>
                        <assign name="Assign10">
                              <copy>
                                    <from variable="GetFlightsOut" part="output"/>
                                    <to variable="GetFlightOut" part="flightList"/>
                              </copy>
                        </assign>
                        <reply name="Reply6" partnerLink="PartnerLink1" operation="getFlight" portType="ns0:TravelGoodWSDLPortType" variable="GetFlightOut"/>
                    </sequence>
                </onMessage>
                  <onMessage partnerLink="PartnerLink1" operation="addFlight" portType="ns0:TravelGoodWSDLPortType" variable="AddFlightIn">
                      <documentation>addFlight operation</documentation>
                      <correlations>
                              <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                        </correlations>
                        <sequence name="Sequence6">
                             <assign name="Assign14">
                                  <copy>
                                       <from>$arrayFlightSize + 1</from>
                                       <to variable="arrayFlightSize"/>
                                  </copy>
                             </assign>
                             <assign name="Assign11">
                                    <copy>
                                          <from>'Flight added'</from>
                                          <to variable="AddFlightOut" part="confirmation"/>
                                    </copy>
                                  <copy>
                                       <from variable="AddFlightIn" part="bookingNumber"/>
                                       <to>$Itinerary/bookingsFlight[$arrayFlightSize]/bookingNumber
                                            <sxed2:editor>
                                                 <sxed2:predicate path="$Itinerary/bookingsFlight[$arrayFlightSize]" source="to"/>
                                            </sxed2:editor>
                                       </to>
                                  </copy>
                                  <copy>
                                       <from>'unconfirmed'</from>
                                       <to>$Itinerary/bookingsFlight[$arrayFlightSize]/bookingStatus
                                            <sxed2:editor>
                                                 <sxed2:predicate path="$Itinerary/bookingsFlight[$arrayFlightSize]" source="to"/>
                                            </sxed2:editor>
                                       </to>
                                  </copy>
                                 <copy>
                                     <from>sxxf:date-less-than($GetFlightIn.flightDate, $Itinerary/itineraryStartDate)</from>
                                     <to variable="changeItineraryStartDate"/>
                                 </copy>
                             </assign>
                              <if name="If2">
                                    <condition>$changeItineraryStartDate</condition>
                                    <assign name="Assign12">
                                          <copy>
                                                <from variable="GetFlightIn" part="flightDate"/>
                                                <to>$Itinerary/itineraryStartDate</to>
                                          </copy>
                                    </assign>
                              </if>
                              <reply name="Reply7" partnerLink="PartnerLink1" operation="addFlight" portType="ns0:TravelGoodWSDLPortType" variable="AddFlightOut"/>
                        </sequence>
                  </onMessage>
                <onMessage partnerLink="PartnerLink1" operation="cancelItinerary" portType="ns0:TravelGoodWSDLPortType" variable="CancelItineraryIn">
                    <documentation>cancelItinerary operation</documentation>
                    <correlations>
                        <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                    </correlations>
                    <sequence name="Sequence10">
                        <assign name="Assign19">
                            <copy>
                                <from>'Planning canceled'</from>
                                <to variable="CancelItineraryOut" part="cancelValidation"/>
                            </copy>
                        </assign>
                        <reply name="Reply8" partnerLink="PartnerLink1" operation="cancelItinerary" portType="ns0:TravelGoodWSDLPortType" variable="CancelItineraryOut"/>
                        <exit name="Exit1"/>
                    </sequence>
                </onMessage>
            </pick>
            <condition>$doneProcess</condition>
        </repeatUntil>
        <!--<repeatUntil name="RepeatUntil2">
            <pick name="Pick2">
                <onMessage partnerLink="PartnerLink1" operation="getItinerary" portType="ns0:TravelGoodWSDLPortType" variable="GetItineraryIn">
                    <correlations>
                        <correlation set="CustomerItineraryCorrelation" initiate="no"/>
                    </correlations>
                    <sequence name="Sequence13">
                        <assign name="Assign22">
                            <copy>
                                <from variable="Itinerary"/>
                                <to variable="GetItineraryOut" part="itinerary"/>
                            </copy>
                        </assign>
                        <reply name="Reply11" partnerLink="PartnerLink1" operation="getItinerary" portType="ns0:TravelGoodWSDLPortType" variable="GetItineraryOut"/>
                    </sequence>
                </onMessage>
            </pick>
            <condition>$doneProcess</condition>
        </repeatUntil>-->
    </sequence>
</process>


















































